
- apiVersion: v1
  kind: DeploymentConfig
  metadata:
    annotations:
      openshift.io/generated-by: OpenShiftNewApp
    creationTimestamp: null
    labels:
      app: semantic
    name: semantic-wiki
  spec:
    replicas: 1
    selector:
      app: semantic
      deploymentconfig: semantic-wiki
    strategy:
      resources: {}
    template:
      metadata:
        annotations:
          openshift.io/generated-by: OpenShiftNewApp
        creationTimestamp: null
        labels:
          app: semantic
          deploymentconfig: semantic-wiki
      spec:
        containers:
        - env:
          - name: MEDIAWIKI_SITE_NAME
            value: wiki-test
          - name: MEDIAWIKI_DB_HOST
            value: mysql
          - name: MEDIAWIKI_DB_USER
            value: wikiuser
          - name: MEDIAWIKI_DB_PASSWORD
            value: wikipassword
          - name: MEDIAWIKI_DB_NAME
            value: mediawiki
          image: library/semantic-wiki:latest
          name: semantic-wiki
          ports:
          - containerPort: 8080
            protocol: TCP
          resources: {}
    triggers:
    - type: ConfigChange
    - imageChangeParams:
        automatic: true
        containerNames:
        - semantic-wiki
        from:
          kind: ImageStreamTag
          name: semantic-wiki:latest
      type: ImageChange
  status: {}
- apiVersion: v1
  kind: Service
  metadata:
    annotations:
      openshift.io/generated-by: OpenShiftNewApp
    creationTimestamp: null
    labels:
      app: semantic
    name: semantic-wiki
  spec:
    ports:
    - name: 8080-tcp
      port: 8080
      protocol: TCP
      targetPort: 8080
    selector:
      app: semantic
      deploymentconfig: semantic-wiki
  status:
    loadBalancer: {}
kind: List
metadata: {}

